{
  "conformance-classes": [
    "http://www.opengis.net/spec/cql/1.0/conf/core",
    "http://www.opengis.net/spec/cql/1.0/conf/spatial",
    "http://www.opengis.net/spec/cql/1.0/conf/temporal"
  ],
  "capabilities": [
    {
      "name": "logical",
      "operators": [
        "and",
        "or",
        "not"
      ]
    },
    {
      "name": "comparison",
      "operators": [
        "lt",
        "lte",
        "eq",
        "gte",
        "gt",
        "neq"
      ]
    },
    {
      "name": "spatial",
      "operators": [
        "equals",
        "disjoint",
        "touches",
        "within",
        "overlaps",
        "crosses",
        "intersects",
        "contains"
      ]
    },
    {
      "name": "temporal",
      "operators": [
        "after",
        "before",
        "begins",
        "begunby",
        "tcontains",
        "during",
        "endedby",
        "ends",
        "tequals",
        "meets",
        "metby",
        "toverlaps",
        "overlappedby",
        "anyinteracts",
        "intersects"
      ]
    },
    {
      "name": "arithmetic",
      "operators": [
        "+",
        "-",
        "*",
        "/"
      ]
    }
  ],
  "functions": [
    {
      "name": "myfunction",
      "returns": {
        "type": "boolean"
      },
      "arguments": [
        {
          "name": "arg1",
          "type": "string"
        },
        {
          "name": "arg2",
          "type": "number"
        },
        {
          "name": "arg3",
          "type": "geometry",
          "typeRef": "https://tools.ietf.org/html/rfc7946#section-3.1"
        }
      ]
    }
  ]
}
